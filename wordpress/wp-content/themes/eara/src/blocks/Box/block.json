{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "eara/box",
  "title": "Box",
  "category": "eara",
  "icon": "format-aside",
  "description": "A simple container box with optional link, background, and spacing.",
  "supports": {
    "html": false,
    "color": {
      "text": true,
      "background": true
    },
    "background": {
      "backgroundImage": true,
      "backgroundSize": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    },
    "dimensions": {
      "aspectRatio": true,
      "height": true,
      "minHeight": true,
      "width": true
    },
    "interactivity": true,
    "layout": true
  },
  "attributes": {
    "href": {
      "type": "string",
      "default": ""
    },
    "borderRadius": {
      "type": "string",
      "default": ""
    },
    "borderColor": {
      "type": "string",
      "default": ""
    },
    "borderWidth": {
      "type": "string",
      "default": ""
    },
    "borderStyle": {
      "type": "string",
      "default": "solid"
    }
  },
  "textdomain": "eara",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css"
}
