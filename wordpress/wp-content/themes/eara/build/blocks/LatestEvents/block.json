  "category": "eara",
  "icon": "calendar-alt",
  "description": "Display the latest events with title, description and category",
  "supports": {
    "html": false
  },
  "example": {},
  "attributes": {
    "postsPerPage": {
      "type": "number",
      "default": 3
    },
    "columns": {
      "type": "number",
      "default": 3
    }
  },
  "textdomain": "eara",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./index.css"
}