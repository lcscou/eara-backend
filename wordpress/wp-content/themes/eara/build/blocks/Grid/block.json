{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "eara/grid",
  "title": "Grid (DEPRECATED)",
  "category": "eara",
  "icon": "grid-view",
  "description": "Flexbox grid system with customizable columns and gaps, like Mantine Grid.",
  "supports": {
    "html": false
  },
  "attributes": {
    "columns": {
      "type": "number",
      "default": 12
    },
    "gutter": {
      "type": "string",
      "default": "md"
    },
    "grow": {
      "type": "boolean",
      "default": false
    },
    "justify": {
      "type": "string",
      "default": "flex-start"
    },
    "align": {
      "type": "string",
      "default": "stretch"
    },
    "overflow": {
      "type": "string",
      "default": "visible"
    }
  },
  "textdomain": "eara",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./index.css"
}