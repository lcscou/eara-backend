{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "eara/google-maps",
  "title": "Google Maps",
  "category": "eara",
  "icon": "location",
  "description": "Embed a Google Maps iframe by providing a location URL",
  "supports": {
    "html": false
  },
  "example": {
    "attributes": {
      "mapUrl": "https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3024.1234567890123!2d-73.9352!3d40.7306!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x0!2zNDDCsDQzJzU0LjYiTiA3M8KwNTYnMDcuOCJX!5e0!3m2!1sen!2sus!4v1234567890",
      "height": "400"
    }
  },
  "attributes": {
    "mapUrl": {
      "type": "string",
      "default": ""
    },
    "height": {
      "type": "string",
      "default": "400"
    },
    "width": {
      "type": "string",
      "default": "100%"
    },
    "title": {
      "type": "string",
      "default": "Location Map"
    },
    "border": {
      "type": "boolean",
      "default": true
    },
    "borderRadius": {
      "type": "string",
      "default": "8"
    }
  }
}